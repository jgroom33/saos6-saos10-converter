[
{% for item in interface_remote_set %}
    {
        "name": "mgmtbr0",
        "admin-status": "true",
        "ipv4": {
            "addresses": [
                {
                    "ip": "{{ item.ip }}"
                }
            ]
        }
    }
{% endfor %}
]

{# 
<interface>
    <name>mgmtbr0</name>
    <admin-status xmlns="http://ciena.com/ns/yang/ciena-openconfig-interfaces">true</admin-status>
    <ipv4 xmlns="http://ciena.com/ns/yang/ciena-openconfig-if-ip">
        <addresses>
            <address>
                <ip>{{ data.IPV4 }}</ip>
                <config>
                    <ip>{{ data.IPV4 }}</ip>
                    <prefix-length>{{ data.CIDR }}</prefix-length>
                </config>
            </address>
        </addresses>
    </ipv4>
</interface> 
#}