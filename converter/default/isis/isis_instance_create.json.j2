[
{% if isis_instance_create|length > 0 %}
  {
    "instance": [
  {% for item in isis_instance_create %}
    {
      {% if item.level != "" %}
        "level-type" : "{{item.level}}",
      {% endif %}
      {% if item.area != "" %}
        "net" : "{{item.area}}.0000.0000.0000.00",
      {% endif %}
      "name": "{{ item.name }}"
    }{% if not loop.last %},{% endif %}
  {% endfor %}
    ]
  }
{% endif %}
]